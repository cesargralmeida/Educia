<!DOCTYPE html>
<html>
  <head>
    <title>Educia</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,700,300italic,400italic,700italic" rel="stylesheet" type="text/css">
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'bootstrap', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'fontawesome', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'stylish-portfolio', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'landing_navbar' %>
    <%= yield %>
    <%= render 'landing_footer' %>
  </body>
</html>
